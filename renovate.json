{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "prHourlyLimit": 3,
  "packageRules": [
    {
      "matchUpdateTypes": ["major"],
      "dependencyDashboardApproval": true
    }
  ]
}
{
  "fileMatch": [
    "\\.html?$"
  ],
  "versioning": "semver",
  "digest": {
    "enabled": false
  },
  "pinDigests": false
}

